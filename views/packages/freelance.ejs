<%- include ('../partials/header')  %>

<div class='freelancerSignUp'>

  

  <form action="/freelancer" method="post">
  <div class='freelancerSignUpContent'>
    <h3 id='freelancerSignUpHeader'>Sign Up as a Freelancer</h3>
      <div class='freelancerInputs'>
        <input class='freelancerInput' type='text' name='freelancerFirstName' placeholder="First Name">
        <input class='freelancerInput' type='text' name='freelancerLastName' placeholder="Last Name">
      </div>
      <div class='freelancerInputs'>
        <input class='freelancerInput' type='text' name='username' placeholder="Email">
        <input class='freelancerInput' type='password' name='password' placeholder="Password">
      </div>
      </form>
      <div class='freelancerInputs'>
         <a href='/freelancer/:id/createprofile'></a> <button class='btn btn-light btn-sm' id='freelancerSignUpButton'>Sign Up!</button>
          </a>
      </div>
  </div>





</div>

<%- include ('../partials/footer')  %>